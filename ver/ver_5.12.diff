+def doAutoAge(dbm, system, cono=None, chain=None, acno=None, pbar=None):
+    from TartanClasses import ASD, Sql
+    tab = "%strn" % system
+    age = "%sage" % system
+    sql = Sql(dbm, [tab, age],  __name__)
+    col = getattr(sql, "%s_col" % tab)
+    if system == "crs":
+        pfx = "crt"
