+    def doHstNam(self, frt, pag, r, c, p, i, w):
+        found = False
+        for host in self.hosts:
+            if w == host[0].strip():
+                found = True
+                break
+        if not found:
+            return "Invalid Terminal"
+        self.acc = self.sql.getRec(tables="posdev", where=[("psd_cono",
+            "=", self.opts["conum"]), ("psd_host", "=", w)], limit=1)
+        if not self.acc:
+            self.new = True
+            if self.locs == "N":
