+        # Draw Report
+        head = ("%03u %-30s" % (self.opts["conum"], self.opts["conam"]))
+        txt1 = "Cash Reconciliation for %s" % self.host
+        txt2 = "Declared by %s (%s) on %s" % (self.name, self.user, self.dated)
