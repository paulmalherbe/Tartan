+                self.df.loadEntry(frt, pag, p+1, data=self.loc)
+                self.df.loadEntry(frt, pag, p+2, data=des[0])
+                return "sk2"
+        else:
+            self.new = False
+            for num, dat in enumerate(self.acc[2:]):
+                self.df.loadEntry(frt, pag, p+num+1, data=dat)
