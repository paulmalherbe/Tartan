+            (("T",0,5,0),"ITX",30,"Printer Name","",
+                "Default","N",self.doPrtNam,prt,None,None),
+            (("T",0,6,0),"IUI",1,"Paper Width (8/6)","",
+                8,"N",self.doPrtWid,None,None,("in", (8, 6))),
+            (("T",0,7,0),"INa",3,"Cut Paper Code","",
