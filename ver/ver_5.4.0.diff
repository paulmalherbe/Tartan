+    def doLocCod(self, frt, pag, r, c, p, i, w):
+        des = self.sql.getRec(tables="strloc", cols=["srl_desc"],
+            where=[("srl_cono", "=", self.opts["conum"]),
+            ("srl_loc", "=", w)], limit=1)
+        if not des:
+            return "Invalid Location"
+        self.loc = w
+        self.df.loadEntry(frt, pag, p+1, data=des[0])
