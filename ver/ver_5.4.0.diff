+        lout = {}
+
+        def doprint(e, lnum):
+            for x in e:
+                lnum += 1
+                lout[lnum] = [x[0]]
+                opt = list(style.element_options(x[0]))
+                opt.sort()
+                for y in opt:
+                    lout[lnum].append(
+                        (y[1:], style.lookup(x[0], y[1:])))
+                if type(x[1]) == dict:
+                    for key in list(x[1].keys()):
+                        if key == "children":
+                            doprint(x[1][key], lnum)
+                        else:
